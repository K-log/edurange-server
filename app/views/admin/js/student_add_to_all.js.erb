<% @student_group_users.each do |student_group_user| %>
    <% if not student_group_user.errors.any? %>
      $('#student-group-<%= @student_group.id %>-table #foot').before(
        '<%= escape_javascript render "/admin/student_group_student", student_group: @student_group, student: student_group_user %>'
      );
    <% end %>
<% end %>
$('#modal-student-group-user-add').modal('hide');
<% if @student_group_users %>
	<% if @student_group.errors.any? %>
	        alert('Error adding user(s) to student group.');
	<% else %>
                <% @student_group_users.each do |sgu| %>
                        <% if sgu.errors.any? %>
                                <% if sgu.errors[:user].any? %>
                                        alert('<%= sgu.user.name %> is already in <%= @student_group.name %>');
                                <% end %>
                        <% else %>
                                $('#student-group-<%= @student_group.id %>-table #foot').before(
                                  "<%= escape_javascript render partial: 'instructor/student_group_table_entry', locals: { sgu: sgu, student_group: @student_group } %>"
                                );
                        <% end %>
                <% end %>
                $('#student-group-<%= @student_group.id %> .collapse').collapse('show');
                $('#student-group-2 :checkbox').prop('checked', false);
	<% end %>
<% else %>
	alert('Could not find student group: <%= @student_group.name %>');
<% end %>

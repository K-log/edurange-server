$('#role-<%= @role.id %>-dropdown').removeClass('open');
<% if @role.errors.any? %>
  <% if @role.errors[:running].any? %>
    alert('<%= @role.errors[:running][0] %>');
  <% end %>
<% else %>
  $('#role-<%= @role.id %>-package-<%= @package.filename_safe %>').remove();
  $('#save-button-li').attr('class', '');
  $('#save-button').attr('href', '/scenarios/<%= @role.scenario.id %>/save');
  update_flash("Removed Package '<%= @package %>' from Role '<%= @role.name %>'.");
<% end %>